{
  "name": "@otak/sequelize-proxy",
  "version": "0.2.9",
  "description": "Sequelize proxy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "format": "tsfmt -r",
    "lint": "tslint -p tsconfig.json",
    "fix": "tslint -p tsconfig.json --fix --quiet && npm run format",
    "test": "jest --forceExit --coverage --verbose --passWithNoTests --detectOpenHandles"
  },
  "keywords": [
    "sequelize",
    "proxy",
    "graphql"
  ],
  "author": "Howard Yang",
  "contributors": [
    "Mathieu Amiot <amiot.mathieu@gmail.com> (https://github.com/OtaK)"
  ],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "dataloader": "^2.0.0"
  },
  "peerDependencies": {
    "sequelize": "*",
    "sequelize-typescript": "*"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/validator": "^13.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.0.0",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5",
    "typescript-formatter": "^7.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {}
    }
  }
}
